<web-app>

  <display-name>Front Controller Example</display-name>

  <servlet>
    <servlet-name>FrontControllerServlet</servlet-name>
    <servlet-class>com.example.controller.FrontControllerServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>FrontControllerServlet</servlet-name>
    <url-pattern>/FrontControllerServlet</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>Login.html</welcome-file>
  </welcome-file-list>

</web-app>
