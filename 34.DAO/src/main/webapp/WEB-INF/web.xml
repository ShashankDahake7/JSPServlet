<web-app>

    <display-name>DAO Example</display-name>

    <servlet>
        <servlet-name>StudentController</servlet-name>
        <servlet-class>com.example.controller.StudentController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>StudentController</servlet-name>
        <url-pattern>/StudentController</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
